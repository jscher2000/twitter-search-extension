{
	"name": "Twitter Search",
	"description": "English-language Twitter search plugin based on the one in Firefox 77.",
	"manifest_version": 2,
	"version": "1.0",
	"applications": {
		"gecko": {
			"id": "twitter-search@jeffersonscher.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Jefferson \"jscher2000\" Scher",
		"url": "https://github.com/jscher2000/"
	},
	"icons": {
		"64": "twitter-search-64.png"
	},
	"web_accessible_resources": [
		"twitter-search-64.png"
	],
	"chrome_settings_overrides": {
		"search_provider": {
			"name": "Twitter",
			"search_url": "https://twitter.com/search?q={searchTerms}"
		}
	}
}